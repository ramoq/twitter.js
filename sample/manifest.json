{
  "manifest_version": 2,
  "name": "TwitterJSExtension",
  "version": "0.0.1",
  "description": "Sample extension using Twitter.js",
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["cs.js"],
      "run_at":"document_end"
    }
  ],
  "web_accessible_resources": [
    "scripts/*.js"
  ]
}

